[core]
	quotepath = false
  editor = vim
  autocrlf = input
[user]
	name = Jonathan Bowers
	email = jonotron@gmail.com
[color]
	ui = auto
[push]
	default = current

[alias]
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
